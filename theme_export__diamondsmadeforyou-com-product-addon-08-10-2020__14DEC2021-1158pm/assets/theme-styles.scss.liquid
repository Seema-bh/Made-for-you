.select-icon {
  position: absolute;
  cursor: pointer;
  width: 25px;
  height: 25px;
  z-index: 3;

  .glyphicon {
    width: 24px;
    height: 24px;
    display: block;
    @include border-radius(50%);
    position: relative;
    cursor: pointer;

    &:before,
    &:after {
      position: absolute;
      top: 50%;
      left: 50%;
      content: "";
      @include translate(-50%, -50%);
    }

    &:before {
      width: 10px;
      height: 2px;
    }

    &:after {
      width: 2px;
      height: 10px;
    }
  }

  &:before{
    -webkit-animation: box-shadowl 1.5s linear infinite;
    animation: box-shadow 1.5s linear infinite;
    content: "";
    width: 24px;
    height: 24px;
    position: absolute;
    @include border-radius(50%);
    left:0;
    top:0;
  }
}


