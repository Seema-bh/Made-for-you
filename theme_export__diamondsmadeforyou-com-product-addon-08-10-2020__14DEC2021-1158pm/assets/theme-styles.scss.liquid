/* ---swap product--- */
.swatch {
  margin-bottom: 11px;

  .header {
    font-size: 10px;
    font-weight: 100;
    line-height: 13px;
    color: #163007;
    margin-bottom: .5rem;
    text-transform: uppercase;
    letter-spacing: 1.5px;
    em {
      color: $background_btn1;
      font-style: inherit;
    }
  }

  input[type=radio],
  .swatch-element .crossed-out {
    display: none;
  }

  .swatch-element {
    @include inline-flex();
    margin: 0 10px 12px 0;
    position: relative;

    &.soldout {
      label {
        cursor: no-drop;
        position:relative;
        &:after {
          display: none;
        }
        .back-souldout{
          position: absolute;
          width: 100%;
          height: 100%;
          left: 0;
          top: 0;
          background: url(soldout-1.png) no-repeat 50%;
        }
      }
    }

    label {
      min-width: 34px;
      height: 34px;
      border: 1px solid #eaeaea;
      margin: 0;
      text-align: center;
      line-height: $font_size + 20;
      color: $color_slick_arrow;
      cursor: pointer;
    }

    .bgImg {
      display: block;
      width: 100%;
      height: 100%;
      background-repeat: no-repeat;
      background-position: center;
      background-size: cover;
    }

    &:not(.color) {
      label {
        background-color:transparent;
        border:none;
      }
    }

    &.color {
      label {
        position: relative;

        &:after {
          position: absolute;
          top: 0;
          left: 0;
          content: "";
          width: 100%;
          height: 100%;
          border: 4px solid $color_slick_arrow;
          background-color: rgba(255, 255, 255, 0);
          z-index: 2;
          @include opacity(0);
          visibility: hidden;
          @include transition(opacity .2s linear);
        }

        &.swatch_variant_img {
          &:after {
            border-width: 2px;
          }
        }
      }
    }

    input:checked {
      &+label {
        background-color: transparent;
        color: #515356;
        font-size: 16px;
        &:after {
          @include opacity(1);
          visibility: visible;
        }
      }
    }
  }

  .tooltip {
    text-align: center;
    background: $background_btn2;
    color: $color_btn2;
    bottom: 100%;
    padding: 5px 10px;
    display: block;
    position: absolute;
    z-index: 2;
    min-width: 50px;
    left: 50%;
    @include translateX(-50%);
    margin-bottom: 12px;
    @include opacity(0);
    visibility: hidden;
    @include transition(all .25s ease-out);
    @include box-shadow(2px, 2px, 6px, rgba(0,0,0,0.28));
    font-size: $font_size;
    font-weight: 500;
    letter-spacing: normal;
    pointer-events: none;

    &:before {
      bottom: -16px;
      content: "";
      display: block;
      height: 16px;
      left: 0;
      position: absolute;
      width: 100%;
    }

    &:after {
      border-left: solid transparent 7px;
      border-right: solid transparent 7px;
      border-top: solid $background_btn2 7px;
      bottom: -7px;
      content: " ";
      height: 0;
      left: 50%;
      @include translateX(-50%);
      position: absolute;
      width: 0;
    }
  }

  .swatch-element:hover .tooltip {
    @include opacity(1);
    visibility: visible;
    pointer-events: auto;
  }
}
