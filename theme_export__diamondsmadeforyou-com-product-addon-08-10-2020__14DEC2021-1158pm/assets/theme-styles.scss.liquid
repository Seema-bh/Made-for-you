/* ---toolbar */
.toolbar{
  @include flexbox();
  @include align-items(center);
  @include flex-wrap(wrap);
  @include justify-content(space-between);
  .column-left{
    margin:33px 0px 0;
    .view-mode{
      @include flexbox();
      @include align-items(left);
      .icon-mode{
        position:relative;
        display:inherit;
        cursor:pointer;
        .icon-tb{
          display: block;
          background: #eeeeee;
        }
        .view-name{
          font-weight:bold;
          font-size:12px;
          letter-spacing:.05em;
          color:#3c3c3c;
          text-transform:uppercase;
          padding: 0px 20px 0px 10px;
          margin-top: 8px;
        }
        &.active{
          .icon-bar,.icon-bar:before,.icon-bar:after{
            background-color:#fff;
          }
          .icon-tb{
            display: block;
            background: #37353c;
          }
        }
      }
    }
    .icon-bar{
      height: 4px;
      background-color: #cfcfcf;
      -webkit-transition: all 0.15s ease;
      -moz-transition: all 0.15s ease;
      -ms-transition: all 0.15s ease;
      -o-transition: all 0.15s ease;
      transition: all 0.15s ease;
      display: block;
      position: relative;
      margin: 1px auto 0;
    }
    .icon-grid{
      padding:10px 16px;
      .icon-bar{
        width:4px;
        &:before,&:after{
          content: '';
          display: block;
          position: absolute;
          height: 4px;
          width: 4px;
          background-color: #cfcfcf;
          -webkit-transition: all 0.15s ease;
          -moz-transition: all 0.15s ease;
          -ms-transition: all 0.15s ease;
          -o-transition: all 0.15s ease;
          transition: all 0.15s ease;
        }
        &:before{
          left:-5px;
        }
        &:after{
          right:-5px;
        }
      }
    }
    .icon-list{
      padding:10px 12px;
      .icon-bar {
        width: 14px;
      }
    }
  }
  .column-right{
    margin:33px 0px 20px;
    @include flexbox();
    @include align-items(left);
    .toolbar-col{
      display:inherit;
      & > label{
        font-size: 12px;
        letter-spacing: 1.8px;
        color: #163007;
        text-transform: uppercase;
        margin-top: 8px;
        margin-right: 13px;
      }
      &:last-child{
        margin-left:30px;
      }
      .limited-view{
        border:1px solid #cbcbcb;
        min-width:120px;
        padding:8px 11px 5px;
        position:relative;
      }
      .filter-sortby{
        min-width: 193px;
        border: transparent;
        padding: 9px 13px;
        position: relative;
        background: #FCFCFC;
        color: #B5AEA5;
        font-size: 14px;
        @media ( max-width: 767px ) {
          min-width: -webkit-fill-available;
        }
      }
      .icon-dropdown{
        position: absolute;
        right: 0px;
        top: 0px;
        padding: 9px 13px;
        cursor: pointer;
      }
    }
    .dropdown-menu{
      transform: translate3d(0px, 38px, 0px)!important;
      border-radius:0!important;
      width: 100%!important;
      min-width: 100%!important;
      -webkit-box-shadow: 2px 3px 0 #cbcbcb;
      -moz-box-shadow: 2px 3px 0 #cbcbcb;
      box-shadow: 2px 3px 0 #cbcbcb;
      li{
        *{
          display: block;
          font-size: 12px;
          padding: 4px 14px 3px 15px;
          cursor: pointer;
          white-space: nowrap;
          overflow: hidden;
          text-overflow: ellipsis;
        }
        &:hover *,&.active *{
          background-color: #f4f4f4;
          color: #66787f;
        }
      }
    }
  }
}
.grid-line{

  clear:both;
  display:block;
  margin:0 auto;
  @include calc(width,'100%');
}
