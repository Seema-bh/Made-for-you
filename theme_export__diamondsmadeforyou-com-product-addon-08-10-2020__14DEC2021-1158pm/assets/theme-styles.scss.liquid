      overflow-y: scroll;
      background-color: {{ settings.bg_menu_mb }};
      z-index: 100;      
      @include transition(transform 0.4s ease);
      @include translateX(-100%);
      transition: opacity 1s cubic-bezier(0.25, 0.46, 0.45, 0.94),height 0.35s cubic-bezier(0.25, 0.46, 0.45, 0.94);
