/***** Search page CSS *****/
.snize-results-page {
  .snize-main-panel {display: none !important;}
  div.snize {
    div.snize-header {margin: 18px 0;}
    div.snize-search-results-header {font-size: 14px;}
    .snize-product-filters-block:hover {background-color: transparent;}
    div.snize-product-filters-title {
      font-size: 17px;
      color: #212529;
      font-family: 'Montserrat Medium';
      text-transform: uppercase;
      padding: 0 0 12px;
      i.snize-product-filters-open {
        width: 10px;
        background: #645e59;
        height: 2px;
        margin-right: 10px;
      }
      &.open {
        i.snize-product-filters-close {
          width: 10px;
          background: #645e59;
          height: 2px;
          margin-right: 10px;
        }
      }
    }
    ul.snize-product-filters-list {
      padding: 0;
      margin-bottom: 30px;
      li:hover .snize-filter-checkbox span {box-shadow: none;}
      .snize-filter-checkbox span {
        width: 16px;
        height: 16px;
        border-radius: 0;
        box-shadow: none;/*0 0 0 1px rgb(208, 208, 208)*/
        border: 1px solid #d0d0d0;
        box-sizing: border-box;
      }
      .snize-filter-checkbox {
        height: 16px;
        width: 16px;
        margin: 0 10px 0 0;
      }
      label > span {
        font-size: 14px;
        padding-left: 24px;
      }
      .snize-filter-checked {
        .snize-filter-checkbox span {
          background: #645e59;
          &:after {border-color: #FFF;}
        }
      }
    }
    #snize-search-results-grid-mode li.snize-product {border-color: #d8d4cf;}
  }
  #snize-search-results-grid-mode {
    span.snize-overhidden {
      background: #fafafa;
      padding: 17px 10px;
      text-align: center;
      display: flex;
      flex-wrap: wrap;
      justify-content: center;
      .snize-title {
        order: 2;
        margin: 0;
        color: #645e59;
        font-size: 12px;
        line-height: 20px;
        font-weight: 400;
      }
    }
    .snize-price-list {
      margin-bottom: 5px;
      display: flex;
      flex-wrap: wrap;
      justify-content: center;
      > span {font-size: 16px;}
    }
    span.snize-discounted-price {
      color: #969696 !important;
      font-style: italic;
      order: 1;
    }
    .snize-price {
      margin-right: 0;
      order: 2;
    }
  }
  @media ( min-width: 768px ) {
    div.snize {
      div.snize-product-filters {
        border-color: #F7F7F7;
        background: #fafafa;
        padding: 38px 15px 20px;
        width: 25%;
      }
      div.snize-search-results-main-content > div {
        width: calc(75% - 20px) !important;
      }
      #snize-search-results-grid-mode li.snize-product {width: 31.33% !important;}
    }
  }
  @media ( max-width: 767px ) {
    div.snize {
      * {box-sizing: border-box;}
    }
    #snize_results.snize-mobile-design.snize-new-version {
      div.snize-search-results-header {
        display: block;
        font-size: 12px;
        margin: 10px 0 14px;
      }
      #snize-search-results-grid-mode {
        ul {
          width: calc(100% + 20px);
          margin: 0 -10px;
        }
        &.snize-two-columns li.snize-product {
          min-width: auto;
          border: none;
          padding: 0 10px;
          &:not(:nth-child(-n+2)) {margin-top: 20px;}
          a.snize-view-link {
            display: block;
            border: 1px solid #d8d4cf;
          }
          span.snize-title {
            color: #645e59;
            font-size: 12px;
            line-height: 20px;
            text-align: center;
            max-height: none !important;
            margin-bottom: 0;
          }
        }
        span.snize-overhidden {
          background: #fafafa;
          padding: 17px 10px;
        }
        .snize-thumbnail-wrapper {padding: 0;}
        span.snize-thumbnail {
          max-width: 100%;
          img {width: 100%;}
        }
        .snize-price-list {
          margin-top: 0px;
          > span {
            font-size: 14px;
            font-weight: 700;
          }
        }
      }
    }
  }
}

@media ( max-width: 767px ) {
  .price-box {
  	font-size: 14px;
    .price-sale {
      display: flex;
      flex-wrap: wrap;
      justify-content: center;
    }
  }
}