/* ----- Someone Purchased ------- */
.product-notification {
  min-height: 100px;
  background-color: $body_bg;
  width: 100%;
  @include transition(all 1s ease);
  @include opacity(0);
  visibility: hidden;
  pointer-events: none;
  @include translateY(-50%);
  @include border-radius(1px);
  border: 1px solid $border_bt_sidebar;

  &.active {
    @include opacity(1);
    visibility: visible;
    pointer-events: auto;
    @include translateY(0);
  }

  .close-notifi {
    position: absolute;
    top: -1px;
    right: 0;
    width: 28px;
    height: 28px;
    @include flexbox();
    @include justify-content(center);
    @include align-items(center);
    background-color: $background_btn2;
    color: $color_btn2;
    border: 1px solid rgba(255, 255, 255, 0);

    &:hover {
      background-color: $background_btn1;
      color: $color_btn1;
    }
  }

  .product-suggest {
    @include flexbox();
    @include justify-content(flex-start);
    @include align-items(center);

    .product-image {
      width: 73px;
      display: block;
      margin: auto;
      padding: 11px 0 9px 13px;

      img {
        object-fit: cover;
      }
    }

    .column-right {
      @include calc(width, '100% - 73px');
      padding: 8px 28px 6px 20px;
      line-height: 20px;

      .info-ago {
        color: $color_compare_price;
        font-size: $font_size - 1;
        margin-top: 7px;
      }

      .time-ago {
        padding-right: 3px;
      }

      .from-ago,
      .time-ago {
        display: inline;
      }

      label {
        display: inline;
        color: $color_compare_price;
        font-size: $font_size;
        font-weight: normal;
        margin: 0;
        padding-right: 3px;
      }

      .product-name {
        display: inline;
        color: $color_title_pr;
        font-size: $font_size;
      }
    }
  }
}
